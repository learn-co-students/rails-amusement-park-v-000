<%= form_for @attraction do |f| %>
  Name: <%= f.text_field :name%><br>
  Ticket Requirement: <%= f.number_field :tickets %><br>
  Height Requirement: <%= f.number_field :min_height %><br>
  Nausea Rating: <%= f.number_field :nausea_rating %><br>
  Happiness Rating: <%= f.number_field :happiness_rating %><br><br>
  <%= f.submit "Create Attraction" %>
<% end %>
