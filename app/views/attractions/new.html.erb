<p>Find me in app/views/attractions/new.html.erb</p>

<%=form_for @attraction do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :tickets %>
  <%= f.text_field :tickets %>

  <%= f.label :nausea_rating %>
