<%= form_for @attraction do |f| %>
 
  <%= f.label :name, "Name" %>
  <%= f.text_field :name%>

  <%= f.label :min_height, "Minimum Height"%>
  <%= f.text_field :min_height%>

  <%= f.label :happiness_rating, "Happiness Rating"%>
  <%= f.text_field :happiness_rating%>

  <%= f.label :nausea_rating, "Nausea Rating"%>
  <%= f.text_field :nausea_rating%>

  <%= f.label :tickets, "Tickets"%>
  <%= f.text_field :tickets%>
 
  <%= f.submit%>
<%end%>