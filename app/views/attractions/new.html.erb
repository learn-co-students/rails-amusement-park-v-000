<h1>Add new attraction:</h1>

<%=form_for @attraction do |f| %>
	<label>Name: </label>
	<%=f.text_field :name %><br>
	<label>Tickets: </label>
	<%=f.text_field :tickets %><br>
	<label>Nausea Rating: </label>
	<%=f.text_field :nausea_rating %><br>
	<label>Happiness Rating: </label>
	<%=f.text_field :happiness_rating %><br>
	<label>Minimium Height: </label>
	<%=f.text_field :min_height %><br>
	<%=f.submit "Create Attraction" %>
<%end%>
