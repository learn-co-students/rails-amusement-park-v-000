<h1>Create New Attraction</h1>

<%= form_for @attraction do |f|%>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :min_height %><br>
  <%= f.text_field :min_height %><br>

  <%= f.label :happiness_rating %><br>
  <%= f.text_field :happiness_rating %><br>

  <%= f.label :nausea_rating %><br>
  <%= f.text_field :nausea_rating %><br>

  <%= f.label :tickets %><br>
  <%= f.text_field :tickets %><br></br>
  
  <%= f.submit %>
<% end %>