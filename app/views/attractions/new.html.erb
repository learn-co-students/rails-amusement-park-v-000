
<%= form_for @attraction do |f| %>
    Name <%= f.text_field :name %>
    Nausea <%= f.number_field :nausea_rating %>
    Happiness <%= f.number_field :happiness_rating %>
    Tickets <%= f.number_field :tickets %>
    Height <%= f.number_field :min_height %>
    <%= f.submit "Create Attraction" %>
    <% end %>