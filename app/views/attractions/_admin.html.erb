<p><%= link_to "New Attraction", new_attraction_path%></p>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Tickers</th>
    <th>Details</th>
  </tr>
    <% @attractions.each do |attraction| %>
  <tr>
    <td><%= link_to attraction.name, attraction_path(attraction) %></td>
    <td><%= attraction.tickets%></td>
    <td><%= link_to "Show #{attraction.name}", attraction_path(attraction)%></td>
  </tr>
  <% end %>
</table>
