Attractions index page

<%@attractions.each do |attraction|%>
    <br><%= attraction.name%> 
    <br><%= attraction.tickets %> 
    <br><%= attraction.nausea_rating %>
    <br><%= attraction.happiness_rating %>
    <br><%= attraction.min_height %>
    <br><%= link_to "Show #{attraction.name}", attraction if admin %> 
    <br><%= link_to "Go on #{attraction.name}", attraction unless admin %>

<%end%>

<%= link_to "New Attraction", new_attraction_path if admin %>