<% @attractions.each do |attraction| %>
  <%= attraction.name %>
  <% if @user.is_admin? %>
    <%= link_to 'Show' + " " + attraction.name, attraction_url(attraction) %>
  <% else %>
    <%= link_to 'Go on' + " " + attraction.name, attraction_url(attraction) %>
  <% end %>
<% end %>

  <% if @user.is_admin? %>
    <%= link_to 'New Attraction', new_attraction_url %>
  <% end %>
