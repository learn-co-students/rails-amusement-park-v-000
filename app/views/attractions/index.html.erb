<h1>Attractions</h1>
  <ul>
    <% @attractions.each do |attraction| %>
      <li>
          <%= link_to "Go on #{attraction.name}", attraction_path(attraction) if @user.admin == false %>


          <%= link_to "Show #{attraction.name}", attraction_path(attraction) if @user.admin == true %>

      </li>

      <% end %>
  </ul>


<% if @user.admin = true %>
<%= link_to 'New Attraction', new_attraction_path%>
<%end%>
