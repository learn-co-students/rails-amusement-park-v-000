
<% if @user && @user.admin? %>
 <% @attractions.each do |attraction| %>
    <%= link_to "Show #{attraction.name}", attraction_path(attraction) %><br>
 <% end %>
    <%= link_to 'New Attraction', new_attraction_path %>
<% else %>
  <% @attractions.each do |attraction| %>
      <%= attraction.name  %>
      <%= attraction.tickets  %>
      <%= link_to "Go on #{attraction.name}", attraction_path(attraction) %>
    <% end %><br>
<% end %>
