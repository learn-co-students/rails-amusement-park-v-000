<% require 'pry' %>
<h1>Attractions</h1>

  <% @attractions.each do |attraction| %>
    <%= link_to "Show #{attraction.name}", attraction if admin %><br>
    <%= link_to "Go on #{attraction.name}", attraction unless admin %><br>
  <%end%>

<% if admin %>
  <%= link_to "New Attraction", new_attraction_path if admin %>
<% end %>



