<p id="notice"><%= notice %></p>

<h1>Attractions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attractions.each do |attraction| %>
      <tr>
        <td>
          <%= index_page_admin_toggle(attraction) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= render 'add_attraction_link' if current_user && current_user.admin? %>
