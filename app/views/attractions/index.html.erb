<h1>Listing Attractions</h1>

<table class="table">
    <thead>
        <tr>
            <th colspan="3"></th>
        </tr>
    </thead>
    <tr>
        <th>Name</th>
        <th>Tickets</th>
        <th>Details</th>
    </tr>

    <% @attractions.each do |att| %>
        <tr>
            <td><%= att.name %></td>
            <td><%= att.tickets %></td>
            <td><%= link_to admin_index_string + att.name, attraction_path(att) %></td>
        </tr>
    <% end %>
    
</table>

<%= link_to "New Attraction", new_attraction_path %>