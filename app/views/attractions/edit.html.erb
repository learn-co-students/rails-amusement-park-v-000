<%= form_for(@a) do |a| %>
  <%= a.label :name %>
  <%= a.text_field :name %>

  <%= a.label :min_height %>
  <%= a.number_field :min_height %>

  <%= a.label :happiness_rating %>
  <%= a.number_field :happiness_rating %>

  <%= a.label :nausea_rating %>
  <%= a.number_field :nausea_rating %>

  <%= a.label :tickets %>
  <%= a.number_field :tickets %>

  <%= a.submit %>
<% end %>
