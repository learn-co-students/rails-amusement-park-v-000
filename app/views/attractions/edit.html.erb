<h1>Create an Attraction! </h1>
<%= form_for(@attraction) do |f| %>
Name: <%= f.text_field :name %><br>
Height: <%= f.number_field :min_height %><br>
Happiness: <%= f.number_field :happiness_rating %><br>
Nausea: <%= f.number_field :nausea_rating %><br>
Tickets: <%= f.number_field :tickets %><br>
<%= f.submit %>
<% end %>
