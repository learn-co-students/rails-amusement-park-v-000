<%= form_for(@attraction) do |f| %>
Name: <%= f.text_field :name, placeholder: "name"  %><br>
Minimum Height: <%= f.text_field :min_height, placeholder: "Minimum Height"  %><br>
Happiness Rating: <%= f.text_field :happiness_rating, placeholder: "Happiness Rating"  %><br>
Nausea Rating: <%= f.text_field :nausea_rating, placeholder: "Nausea Rating"  %><br>
Tickets: <%= f.text_field :tickets, placeholder: "Tickets"  %><br>
<%= f.submit "Update Attraction" %><br>
<% end %>
