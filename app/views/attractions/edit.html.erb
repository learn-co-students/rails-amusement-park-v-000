 # edits the attraction form

 <section class="todoapp">
  <header class="header">
  
    <h1>New Attraction</h1>
    ## this route to the users controller
    <%= form_for(@attraction) do |f| %>
       <%= f.label :name %>:
       <%= f.text_field :name %><br />

      <%= f.label :min_height %>:
        <%= f.text_field :min_height %><br />

        <%= f.label :nausea_rating %>:
        <%= f.text_field :nausea_rating %><br />

        <%= f.label :happiness_rating %>:
        <%= f.text_field :happiness_rating %><br />

        <%= f.label :tickets %>:
        <%= f.text_field :tickets %><br />

       
        <br>
        <%= f.submit%>
        <% end %>
      </form>

  </header>
  
  <footer class="footer">
  </footer>
</section>
