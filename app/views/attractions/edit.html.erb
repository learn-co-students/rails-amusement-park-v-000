"Toggle navigation Welcome Name:" <% @attraction.name  - Edit Attraction %>

<%= form_for @attraction do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>

  <%= f.label :nausea_rating %>
  <%= f.number_field :nausea_rating %><br><br>

  <%= f.label :happiness_ratint %>
  <%= f.number_field :happiness_rating %><br><br>

  <%= f.label :min_height %>
  <%= f.number_field :min_height %><br><br>

  <%= f.label :tickets %>
  <%= f.number_field :tickets %><br><br>

<%= f.submit 'Edit Attraction' %>

<% end %>

<% if current_user.admin %>
  <%= link_to "Edit Attraction" @attraction.name, @attraction %>
<% end %>
