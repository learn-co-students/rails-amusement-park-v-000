<%=@attraction.name%>

<% if User.find(session[:user_id]).admin == true %>
  <%=link_to("Edit Attraction", edit_attraction_path(@attraction))%>
<%end%>

<% if User.find(session[:user_id]).admin == false %>
  <%= form_for @attraction do |f| %>
    <%= f.button("Go on this ride") %>
  <% end %>
<%end%>
