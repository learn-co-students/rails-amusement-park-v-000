<%= form_for @attraction do |f| %>
  <label>Name: </label><%= f.text_field :name %></br>
  <label>Min Height: </label><%= f.text_field :min_height %></br>
  <label>Happiness Rating: </label><%= f.text_field :happiness_rating %></br>
  <label>Nausea Rating: </label><%= f.text_field :nausea_rating %></br>
  <label>Tickets: </label><%= f.text_field :tickets %></br>
  <%= yield f %>
<% end %>