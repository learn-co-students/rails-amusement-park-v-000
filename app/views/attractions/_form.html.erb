<div class="container form-group">

<%= form_for(attraction) do |f| %>
	Name: <%= f.text_field :name %><br>
	Height Requirement: <%= f.number_field :min_height %><br>
	Happiness Rating(1-5):  <%= f.number_field :happiness_rating %><br>
	Nausea Rating(1-5): <%= f.number_field :nausea_rating %><br>
	Tickets: <%= f.number_field :tickets %><br>
	<%= f.submit %>
<% end %>
  
 </div>