<h1>Sign Up</h1>

<%= form_for @user do |f| %>
    <%= f.text_field :name %>
    <%= f.text_field :height %>
    <%= f.text_field :happiness %>
    <%= f.text_field :nausea %>
    <%= f.text_field :tickets %>
    <%= f.check_box :admin %>
    <%= f.password_field :password %>
    <%= f.submit 'Create User'%>
<% end %>