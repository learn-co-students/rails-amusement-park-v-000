<%=form_for @user do |f| %> 
  <%=f.label :name %> <br/>
  <%=f.text_field :name %> <br/> 

  <%=f.label :happiness %> <br/>
  <%=f.number_field :happiness%> <br/> 

  <%=f.label :nausea %> <br/>
  <%=f.number_field :nausea%> <br/>

  <%=f.label :height %> <br/>
  <%=f.number_field :height%> <br/>

  <%=f.label :tickets %> <br/>
  <%=f.number_field :tickets%> <br/>

  <%=f.label :admin %> </br>
  <%=f.check_box :admin %>

  <%=f.label :password %> <br/>
  <%=f.password_field :password%> <br/> 

  <%=f.submit%>


<%end%>
