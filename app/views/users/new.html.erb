<%= form_for @user do |f| %>
  <%=f.label :name %>
  <%=f.text_field :name %>
  <%=f.label :password %>
  <%=f.text_field :password %>
  <%=f.text_field :height %>
  <%=f.text_field :nausea %>
  <%=f.text_field :happiness %>
  <%=f.text_field :tickets %>
  <%=f.check_box :admin %>
  <%=f.submit 'Create User'%>
<%end%>
