<%= form_for @user, controller: 'users', action: 'create' do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
  
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :nausea %>
    <%= f.text_field :nausea %>

    <%= f.label :happiness %>
    <%= f.text_field :happiness %>

    <%= f.label :tickets %>
    <%= f.text_field :tickets %>

    <%= f.label :height %>
    <%= f.text_field :height %>

    <%= f.label :admin, "Check box if you are a theme park owner" %>
    <%= f.check_box :admin%>
  
    <%= f.submit "Create User" %>
  <% end %>

