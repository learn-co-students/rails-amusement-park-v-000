<%= form_for (@user) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :height %>
<%= f.text_field :height %>
<%= f.label :happiness %>
<%= f.number_field :happiness %>
<%= f.label :nausea %>
<%= f.number_field :nausea %>
<%= f.label :tickets %>
<%= f.number_field :tickets %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.submit %>
<% end %>
