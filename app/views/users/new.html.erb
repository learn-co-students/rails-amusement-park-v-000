<!-- <%= render 'form'%>  -->
<h1>Sign Up</h1>


<%= form_for @user do |f| %>
  <%=f.label :name %>
  <%= f.text_field :name %>
  <%=f.label :height %>
  <%=f.number_field  :height %>
  <%= f.label :nausea %>
  <%= f.number_field  :nausea %>
  <%= f.label :tickets %>
  <%= f.number_field :tickets %>
  <%=f.label :happiness %>
  <%=f.number_field :happiness%>
  <%=f.label :Check_box_if_you_are_a_theme_park_owner %>
  <%=f.check_box :admin %>
  <%=f.label :password %>
  <%=f.password_field :password %>

  <%= f.submit %>
<%end%>
