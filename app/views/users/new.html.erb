<%= form_for @user, controller: 'users', action: 'create' do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
  
    <%# f.label :password_confirmation %>
    <%# f.password_field :password_confirmation %>
    <br>
    <br>
    <%= f.label :nausea %>
    <%= f.text_field :nausea %>
    <br>
    <br>
    <%= f.label :happiness %>
    <%= f.text_field :happiness %>
    <br>
    <br>
    <%= f.label :tickets %>
    <%= f.text_field :tickets %>
    <br>
    <br>
    <%= f.label :height %>
    <%= f.text_field :height %>
    <br>
    <br>
    <%= f.label :admin, "Check box if you are a theme park owner" %>
    <%= f.check_box :admin%>
    <br>
    <br>
    <%= f.submit "Create User" %>
  <% end %>

  