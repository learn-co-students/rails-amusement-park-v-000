

<% if flash[:notice] %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>

<h1><%= @user.name %></h1>
<% if @user.admin? %>
  <%= "ADMIN" %>
<% end %>

<h2>Mood: <%= @user.mood unless @user.nausea.nil? %></h2>
<h2>Tickets: <%= @user.tickets %></h2>
<h2>Height: <%= @user.height %></h2>

<%= link_to "Log Out", '/logout' %><br>
<%= link_to "See attractions", attractions_path %>
