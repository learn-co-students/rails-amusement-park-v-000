<h1>Welcome <%= @user.name %></h1>
<% if flash[:message] %>
    <%= flash[:message] %></div>
<% end %>

 <% if @user.admin %>
   <p>ADMIN</p>
<% else %>
  <p>Name: <%= @user.name %></p><br>
  <p>Tickets: <%= @user.tickets %></p><br>
  <p>Mood: <%= @user.mood %></p><br>
  <p>Height: <%= @user.height %></p><br>
<% end %>
<%= link_to "See attractions", attractions_path %>

 <%= link_to "Log Out", logout_path, method: :post %>
