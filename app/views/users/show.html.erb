<h1>Welcome, <%= @user.name %>!</h1>
<% if @user.admin %>
  <h2>ADMIN USER</h2><br>
<% end %>
<br />
<% if @message %>
  <b><%= @message %></b><br>
<% end %>

<% if @user.mood %> <h2>Mood: <%= @user.mood %></h2> <% end %>
<h2>Tickets: <%= @user.tickets %></h2>
<h2>Height: <%= @user.height %></h2>
<br />
<%= link_to "See attractions", attractions_path %>
<br />
<%= link_to "Log Out", signout_path %>
