<% if flash[:notice] %>
   <div class="notice"><%= flash[:notice] %></div>
 <% end %>

<%byebug%>
<h2> Here is your information <%= @user.name %></h2>
 <%if @user.admin %>
   <li>User Type: ADMIN<li>
 <% end %>


  <%if !@user.admin %>
    <ul>
      <li>Height: <%=@user.height%></li>
      <li>Mood: <%=@user.mood%></li>
      <li>Nausea Level: <%=@user.nausea%></li>
      <li>Tickets: <%=@user.tickets%></li>
    </ul>
  <%end%>

  <%= link_to "See attractions", "/attractions" %>
