<p id="notice"><%= notice %></p>

<% if @message %>
  <div class="center alert">
    <p><%= @message %></p>
  </div>
<% end %>

<% if @user.admin %>
  <h4>ADMIN</h4>
<%end%>

<p>Name: <%= @user.name %></p>
<p>Tickets: <%= @user.tickets %></p>
<p>Height: <%= @user.height %></p>
<p>Mood: <%= @user.mood %></p>
<p><%= link_to 'Edit', edit_user_path(@user) %></p>
<p><%= link_to 'See attractions', attractions_path %></p>
