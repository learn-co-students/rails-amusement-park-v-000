<h1>Hi! <%=@user.name %></h1>
<h3><%= "ADMIN" if @user.admin %></h3>

<h2><%= flash[:notice] if flash[:notice] %></h2>

<p>Name: <%= @user.name %></p>
<p>Height: <%= @user.height %></p>
<p>Tickets: <%= @user.tickets %></p>
<p>Happiness: <%= @user.happiness %></p>
<p>Nausea: <%= @user.nausea %></p>
<p>Mood: <%= @user.mood %></p>

<%= link_to "See attractions", attractions_path %><br>
<%= link_to "Log Out", user_path, method: "delete" %>
