<% flash.each do |type, msg| %>
    <div>
      <%= msg %>
    </div>
  <% end %>

<h1><%= @user.name %><h1>
<h3>Mood: <%= @user.mood %></h3>
<h3>Tickets: <%= @user.tickets %></h3>
<h3>Height: <%= @user.height %></h3>
<% if @user.admin %>
    <h3> ADMIN </h3>
<% end %>

<%= link_to "See attractions", "/attractions" %>
<%= link_to "Log Out", "/logout" %>