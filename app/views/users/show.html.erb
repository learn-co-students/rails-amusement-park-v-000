<h1><%= @user.name %> </h1>

<% if @user.admin %>
  <h3>ADMIN</h3>
<% end %>

<ul>
  <%= user_info_li(@user, :mood) %>
  <%= user_info_li(@user, :height) %>
  <%= user_info_li(@user, :tickets) %>
</ul>

<a href="<%=attractions_url%>">See attractions</a>

<% if flash[:alert] %>
  <p><%= flash[:alert] %></p>
<% end %>
