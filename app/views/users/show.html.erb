<% if @message %>
    <p><%= @message %></p>
<% end %>

<% if admin %>
<h3>ADMIN</h3>
<% end %>

<h3>Name: <%=  @user.name %></h3><br>

<% if !admin %>
  <h3>Tickets: <%=  @user.tickets %></h3><br>
  <h3>Mood: <%= @user.mood %></h3><br>
  <h3>Height: <%= @user.height %></h3><br>
<% end %>

<%= link_to 'See attractions', attractions_path %>
