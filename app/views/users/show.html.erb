<div class="center">
  <% flash.each do |name, msg| %>
    <%= msg %><br>
  <% end %>
  <div class="info">
    <% if current_user.admin %>
      <p><strong>ADMIN</strong>
    <% end %>
    <p><strong>Name:</strong> <%= current_user.name %></p>
    <p><strong>Tickets:</strong> <%= current_user.tickets %></p>
    <p><strong>Mood:</strong> <%= current_user.mood %></p>
    <p><strong>Height:</strong> <%= current_user.height %> inches</p>
    <p><%= link_to "See attractions", attractions_url %></p>
  </div>
</div>
