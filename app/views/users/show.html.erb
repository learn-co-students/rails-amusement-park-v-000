<h1>
  Name:
  <%=@user.name%></h1>
<% if @message %>
  <div class="center alert">
    <p><%= @message %></p>
  </div>
<% end %>
<% if !@user.admin%>
  <h3>
    Mood:
    <%=@user.mood%></h3>
  <h3>
    Tickets:
    <%=@user.tickets%></h3>
  <h3>
    Height:
    <%=@user.height%></h3>
<%else%>
  <h2>ADMIN</h2>
<% end %>
<h3><%= link_to 'See attractions', attractions_path%></h3>
