
<%= form_for @user, :url => url_for(controller: 'sessions', action: 'create') do |f| %>
    <%= f.label :user_name %>
    
    <%= f.collection_select :name, User.all, :name, :user_name %>
    
    <%= f.label :password %>
    <%= f.password_field :password%>
   
    <%= f.submit  "Sign In"%>
<% end %>