<h1>Returning Users Signin</h1>



<%= form_tag(method: :post, action: :create, controller: :sessions) do %>

<label>Username</label><br>
<%= collection_select :user, :name, User.all, :id, :name %><br>

<label>Password</label><br>
<%= text_field_tag :password %><br>
<%= submit_tag "Sign In" %>
<% end %>
