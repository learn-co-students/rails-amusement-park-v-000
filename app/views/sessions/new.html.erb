<%= form_tag sessions_path do %>
  <%= label_tag "name", "Name" %><br>
  <%= collection_select(:user, :name, User.all, :name, :name, prompt: "Please select your username") %><br>
  <%= label_tag "password", "Password" %><br>
  <%= password_field_tag "password" %><br>
  <%= submit_tag "Sign In" %>
<% end %>

