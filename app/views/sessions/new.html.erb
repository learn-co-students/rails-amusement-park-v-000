<%= form_for @user, :url => signin_path do |f|  %>

    
    <%# key attribute for the parameter, what's being collected, attribute of the collection that's being collected, what's being displayed. %>

	Name: <%= f.collection_select(:name, @users, :name, :name) %>

    Password: <%= f.password_field :password %>


    <%= f.submit "Sign In"%>

<% end %>


