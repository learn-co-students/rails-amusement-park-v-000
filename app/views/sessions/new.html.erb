<%= form_for @user, url: signin_path(@user) do |f| %>
  <%= f.label :name, "Select your name"%>
  <%= f.collection_select(:name, User.all, :name, :name, prompt: "Please select username") %><br>

  <%= f.label :password, "Enter your password" %>
  <%= f.password_field :password%>
  <%= f.submit "Sign In" %>
<% end %>
