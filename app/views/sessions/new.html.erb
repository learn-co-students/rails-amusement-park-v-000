<h3> Log In </h3>

<%= form_for (@user), url: signin_path, method: 'post' do |f| %>

<%= f.label :name%>
<%= f.collection_select(:name, User.all, :name, :name)%> <br><br>

<%= f.label :password %>
<%= f.password_field :password, id: 'password' %><br> <br>

<%= f.submit "Sign In"  %><br><br>
<% end %>
