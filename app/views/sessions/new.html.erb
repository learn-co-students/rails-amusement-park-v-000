<h3> Please login!</h3>

<%= form_tag(signin_path) do %>
<%= collection_select :user, :name, User.all, :name, :name, :include_blank => "select"   %>
   </br>
   <%= label_tag :password %>
   <%= password_field_tag :password %>
   </br>
   <%= submit_tag "Sign In" %>
<% end %>

<%= link_to "Sign Up", new_user_path %>
