<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>
  
 <%= label_tag(:name, "Name:") %>
  <%= select_tag "name", options_from_collection_for_select(@users, "name", "name"), id: 'user_name' %>
<%= label_tag(:password, "Password:") %>
  <%= password_field_tag(:password) %>


  <%= submit_tag("Sign In") %>

  
<% end %>