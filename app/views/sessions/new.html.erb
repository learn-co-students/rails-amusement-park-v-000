<%= form_tag "/signin", method: "post" do %>
<label><strong>Your name:</strong></label>
  <%= select_tag 'user_name', options_from_collection_for_select(User.all, 'id', 'name') %>
  <br>
  <label><strong>Your password:</strong></label>
  <%= password_field_tag :password%>
  <%= submit_tag "Sign In" %>
  <%end%>
