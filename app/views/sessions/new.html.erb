<h1>Sign In</h1>
<%= form_tag sessions_path, method: :post, authenticity_token: true do %>
  <%= collection_select :user, :name, User.all, :name, :name, {prompt: "Please select name"}%>
  <%= label_tag :password, "Password" %><br>
  <%= password_field_tag :password %><br>
  <%= submit_tag "Sign In"%>
<% end %>