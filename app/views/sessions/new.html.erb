<h1>Login</h1>

<%= form_tag signin_path do %>
    <%= label_tag :name %></br>
    <%= select_tag :user_name, options_from_collection_for_select(User.all, "id", "name") %></br>
    <%= label_tag :password %></br>
    <%= password_field_tag :password%></br>
    </br>
    <%= submit_tag "Sign In" %>
<% end %>