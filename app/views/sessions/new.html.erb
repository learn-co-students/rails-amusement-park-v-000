<h1>Sign In</h1>

<%= form_with(url: sessions_path, method: :post) do %>
<div class="field">
<label for="name">Name</label><br>
<%= select_tag('user_name', options_from_collection_for_select(@users, 'name', 'name')) %>
</div>

<div class="field">
<label for="password">Password</label><br>
<%= password_field_tag :password %>
</div>

<%= submit_tag "Sign In" %>

<% end %>