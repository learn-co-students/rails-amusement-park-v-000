<%= form_for @user, url: url_for(controller: 'sessions', action: 'create') do |f| %>
  <%= f.label :name %>
  <%= f.select :name, User.all.collect{|user| user.name}, {include_blank: 'None'} %>
  <%= f.label :password %>
  <%= f.password_field :password, id: "password" %>
  <%= f.submit "Sign In" %>
<% end %>
