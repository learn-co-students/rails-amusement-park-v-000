<%= form_for :user, url: '/signin' do |f| %>
  <div class="form-group">
    <%= f.label :user_name, 'Your name:' %> <br>
    <%= collection_select nil, :user_name, @users, :id, :name, prompt: 'Please select your username from this dropdown' %>
  </div>

  <div class="form-group">
    <%= f.label :password, 'Your password:' %><br>
    <%= password_field_tag :password, nil, class: 'form-control' %>
  </div>
  <%= f.submit value: 'Sign In', class: 'btn btn-primary'%>
<% end %>
