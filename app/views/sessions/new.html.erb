<h1>Log In</h1>


<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>
  <%=label_tag "Please select your username."%>
  <%=collection_select :user, :name, User.all, :name, :name%>
  <%=label_tag "Password"%>
  <%=password_field_tag :password %>
  <%= submit_tag "Sign In"%>
<% end %>
